{
  "name": "game-bucket",
  "version": "6.0.0",
  "description": "Common game utilities + rollup config, suitable for a small game competition. Like [JS13k](http://js13kgames.com/)!",
  "files": [
    "dist"
  ],
  "dependencies": {
    "pocket-physics": "^8.0.2",
    "science-halt": "^0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-numeric-separator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-url": "^5.0.1",
    "@types/jest": "^28.1.6",
    "ect-bin": "^1.4.1",
    "gh-pages": "^3.1.0",
    "html-minifier": "^4.0.0",
    "http-server": "^0.12.3",
    "jest": "^28.1.3",
    "prettier": "2.7.1",
    "roadroller": "^2.1.0",
    "rollup": "^2.26.5",
    "rollup-plugin-copy": "^3.3.0",
    "svgo": "^1.3.2",
    "terser": "^5.2.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "types": "tsc",
    "build": "rollup -c",
    "start": "./tools/start.sh",
    "zip": "./tools/zip.sh",
    "deploy": "yarn zip && gh-pages -d dist/",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "author": "Drew Petersen <senofpeter@gmail.com>",
  "license": "MIT",
  "//browserslist": "Prevent babel from over-transpiling anything",
  "browserslist": [
    "chrome 75"
  ]
}
