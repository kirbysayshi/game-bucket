{
  "name": "game-bucket",
  "version": "6.0.0",
  "type": "module",
  "private": true,
  "description": "Common game utilities + rollup config, suitable for a small game competition. Like [JS13k](http://js13kgames.com/)!",
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@9.1.0",
  "dependencies": {
    "pocket-physics": "^11.0.0",
    "science-halt": "^0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-url": "^8.0.1",
    "@types/jest": "^29.5.3",
    "ect-bin": "^1.4.1",
    "gh-pages": "^6.0.0",
    "html-minifier": "^4.0.0",
    "http-server": "^14.1.1",
    "jest": "^29.6.2",
    "prettier": "3.0.3",
    "prettier-2": "npm:prettier@^2",
    "punch-bench": "^4.0.1",
    "roadroller": "^2.1.0",
    "rollup": "^3.28.0",
    "rollup-plugin-copy": "^3.4.0",
    "svgo": "^3.0.2",
    "terser": "^5.19.2",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "types": "tsc",
    "build": "rollup -c",
    "start": "./tools/start.sh",
    "zip": "./tools/zip.sh",
    "deploy": "yarn zip && gh-pages -d dist/",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "author": "Drew Petersen <senofpeter@gmail.com>",
  "license": "MIT",
  "//browserslist": "Prevent babel from over-transpiling anything",
  "browserslist": [
    "firefox 100, last 1 iOS major versions"
  ]
}
