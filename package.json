{
  "name": "game-bucket",
  "version": "5.0.2",
  "description": "Common game utilities + rollup config, suitable for a small game competition. Like [JS13k](http://js13kgames.com/)!",
  "files": [
    "dist"
  ],
  "dependencies": {
    "naivedux": "^1.1.0",
    "pocket-physics": "^4.0.0",
    "science-halt": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "gh-pages": "^2.2.0",
    "http-server": "^0.12.3",
    "rollup": "^2.9.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-url": "^3.0.1",
    "svgo": "^1.3.2",
    "terser": "^4.6.13",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "types": "tsc",
    "build": "rollup -c",
    "start": "./tools/start.sh",
    "zip": "./tools/zip.sh",
    "deploy": "yarn zip && gh-pages -d dist/",
    "clean": "rm -rf dist"
  },
  "author": "Drew Petersen <senofpeter@gmail.com>",
  "license": "MIT",
  "//browserslist": "Prevent babel from over-transpiling anything",
  "browserslist": [
    "chrome 75"
  ]
}
